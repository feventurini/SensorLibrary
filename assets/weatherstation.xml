<?xml version="1.0" encoding="UTF-8"?>
<station name="balcone" xmlns="http://www.w3schools.com"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.w3schools.com stationSchema.xsd">
	
	<provider ip="192.168.0.12" port="1099"/>
	
	<sensor loadAtStartup="false">
		<name>tempFuori</name>
		<class>sensor.implementations.TempAndHumiditySensor</class>
		<parameters>assets/TempAndHumiditySensor.properties</parameters>
	</sensor>
	<sensor loadAtStartup="true">
		<name>meteo</name>
		<class>sensor.implementations.Wunderground</class>
		<parameters>assets/Wunderground.properties</parameters>
	</sensor>
	<sensor loadAtStartup="false">
		<name>display</name>
		<class>sensor.implementations.GroveRgbDisplay</class>
		<parameters></parameters>
	</sensor>
</station>