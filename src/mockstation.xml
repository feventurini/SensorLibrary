<?xml version="1.0" encoding="UTF-8"?>
<station name="cucina" xmlns="http://www.w3schools.com"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.w3schools.com stationSchema.xsd">
	
	<provider ip="192.168.0.12" port="1099"/>
	
	<sensor class="sensor.implementations.Rfid_SL030">
		<name>rfid</name>
		<parameters>rfid.properties</parameters>
		<loadAtStartup>true</loadAtStartup>
	</sensor>
	<sensor class="sensor.implementations.TempAndHumiditySensor">
		<name>temp</name>
		<parameters>TempAndHumiditySensor.properties</parameters>
		<loadAtStartup>true</loadAtStartup>
	</sensor>
	<sensor class="sensor.implementations.GroveRgbDisplay">
		<name>display</name>
		<parameters></parameters>
		<loadAtStartup>true</loadAtStartup>
	</sensor>
</station>