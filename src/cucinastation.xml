<?xml version="1.0" encoding="UTF-8"?>
<station name="cucina" xmlns="http://www.w3schools.com"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.w3schools.com stationSchema.xsd">
	
	<provider ip="192.168.0.12" port="1099"/>
	
	<sensor loadAtStartup="true">
		<name>rfidFrigo</name>
		<class>sensor.implementations.Rfid_SL030</class>
		<parameters>rfid.properties</parameters>
	</sensor>
	<sensor loadAtStartup="true">
		<name>tempAmbiente</name>
		<class>sensor.implementations.TempAndHumiditySensor</class>
		<parameters>TempAndHumiditySensor.properties</parameters>
	</sensor>
	<sensor loadAtStartup="true">
		<name>display</name>
		<class>sensor.implementations.GroveRgbDisplay</class>
		<parameters></parameters>
	</sensor>
</station>